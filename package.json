{
  "name": "chat",
  "version": "1.0.1",
  "description": "app chat",
  "scripts": {
    "dev": "webpack serve --open --config webpack/webpack.dev.js",
    "build": "webpack --config webpack/webpack.prod.js",
    "serve": "node server.js",
    "start": "npm run build && npm run serve",
    "lint": "eslint src --ext .js,.ts && npx stylelint \"**/*.scss\"",
    "fix": "eslint src --ext .js,.ts --fix && npx stylelint \"**/*.scss\" --fix",
    "test": "mocha -r ts-node/register -r ignore-styles -r mock-local-storage -r jsdom-global/register src/**/*.spec.ts --timeout 10000"
  },
  "author": "a.anikin",
  "license": "ISC",
  "browserslist": [
    "> 0.25%, not dead"
  ],
  "staticFiles": {
    "staticPath": "static/files",
    "staticOutPath": "static"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/express": "^4.17.13",
    "@types/jsdom": "^16.2.13",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "chai": "^4.3.4",
    "css-loader": "^6.5.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.25.2",
    "file-loader": "^6.2.0",
    "html-entities": "^2.3.2",
    "html-webpack-plugin": "^5.5.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "18.1.0",
    "jsdom-global": "3.0.2",
    "mini-css-extract-plugin": "^2.4.5",
    "mini-svg-data-uri": "^1.4.3",
    "mocha": "^9.1.3",
    "mock-local-storage": "^1.1.17",
    "mock-xmlhttprequest": "^7.0.4",
    "sass": "^1.43.5",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.0.1",
    "stylelint-config-recommended-scss": "^5.0.0",
    "svg-inline-loader": "^0.8.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "webpack": "^5.64.3",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "express-rate-limit": "^5.5.1",
    "helmet": "^4.6.0"
  }
}
